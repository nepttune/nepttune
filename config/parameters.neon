parameters:
    domain: 'peldax.com'
    sessionName: 'peldaxcom'

    meta:
        name: 'peldax.com'
        nameShort: 'PLX'
        version: '1.0'
        description: 'peldax.com - personal multi-purpose webpage'
        keywords: 'personal, webpage, peldax, application'
        author: 'peldax.com'
        robots: 'index, follow'
        viewport: 'width=device-width, initial-scale=1, maximum-scale=2'
        color: '#ffffff'
        
    robots:
        all:
            name: '*'
            disallow:

    security:
        contact: 'peldax@gmail.com'
        acknowledgements: 'https://github.com/nepttune/nepttune/graphs/contributors'

    dest:
        homepage: ':Www:Default:default'
        
    router:
        defaultModule: 'Www'
        hashids: false
        hashidsSalt: 'nGXWFaJfxlMCkn9aU8XK'
        hashidsPadding: 10
        hashidsCharset: 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890'
